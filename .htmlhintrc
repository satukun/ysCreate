{
    "tagname-lowercase"        : true,//タグ名がLowerCaseか
    "attr-lowercase"           : true,//属性名がLowerCaseか
    "attr-value-double-quotes" : false,//属性の値がダブルクォートでかこまれているか
    "doctype-first"            : false,//DOCTYPE宣言が先頭にあるか
    "tag-pair"                 : true,//閉じタグが正しいか
    "spec-char-escape"         : true,//特殊文字を使用しているか
    "id-unique"                : true,//IDはユニークか
    "src-not-empty"            : true,//srcの値があるか
    "attr-no-duplication"      : true,//同じ属性が複数指定されていないか
    "title-require"            : true,//タイトルタグの中身があるか
    "alt-require"              : true,//alt属性はあるのか
    "attr-value-not-empty"     : true,//属性は値を設定する必要があります。
    "attr-no-duplication"      : true,//同じ属性を2回指定することはできません。
    "tag-self-close"           : false,//空のタグは自己によって閉じなければなりません。
    "head-script-disabled"     : false,//スクリプトタグは頭に使用できません
    "doctype-html5"            : false,//Doctypeはhtml5でなければなりません。
    "id-class-value"           : false,//IDとクラス値はいくつかのルールを満たさなければなりません：下線、ダッシュ、ハンプ。
    "style-disabled"           : true,//スタイルタグは使用できません。
    "inline-style-disabled"    : true,//インラインスタイル無効
    "inline-script-disabled"   : false,//インラインスクリプト無効
    "space-tab-mixed-disabled" : true,//スペースとタブが混ざっているかどうか
    "id-class-ad-disabled"     : false,
    "href-abs-or-rel"          : false,//Hrefは絶対または相対でなければなりません
    "attr-unsafe-chars"        : false//属性値は安全でない文字を使用できません。
}
